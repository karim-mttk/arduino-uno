#include <stdio.h>

int main()
{
    printf("karim \n");
    printf("muttn \n");
    
    printf(" *\n **\n ***\n ****\n *****\n");
    
    int a, b;
    int sum;
    
    a= 10;
    b= 5;
    
    
    sum= a + b;
    
    printf("The sum of %d and %d is %d \n", a, b, sum);
    
    int c, d;
    int diff;
    
    printf("Enter first number \n");
    scanf("%d", &c);
    
    printf("Enter second number\n");
    scanf("%d", &d);
    
    diff = c - d;
    
   printf("The difference between %d and %d is %d", c, d, diff);
   
   
   return 0;
}
